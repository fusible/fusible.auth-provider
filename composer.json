{
    "name": "fusible/auth-provider",
    "description": "Aura\\Auth service provider",
    "homepage":"https://github.com/fusible/fusible.auth-provider",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jake johns",
            "email": "jake@jakejohns.net"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "aura/auth": "^2.0",
        "container-interop/service-provider": "^0.4.0"
    },
    "autoload": {
        "psr-4": {
            "Fusible\\AuthProvider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fusible\\AuthProvider\\":"tests/",
            "Aura\\Di\\": "vendor/aura/di/tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpstan/phpstan": "^0.11",
        "pds/skeleton": "^1.0",
        "aura/di": "^3.4",
        "phpunit/phpunit": "^8.1"
    }
}
